{
  "name": "@jakedeichert/create-app",
  "version": "1.0.2",
  "description": "A web app generator cli that abstracts away build configuration",
  "author": "Jake Deichert",
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "create-app": "bin/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "npm run _prettier:config -- --write '**/*.js'",
    "format:check": "npm run _prettier:config -- --list-different '**/*.js'",
    "_prettier:config": "prettier --single-quote --trailing-comma es5 --jsx-bracket-same-line"
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.5.3",
    "request": "^2.81.0",
    "yargs": "^8.0.2",
    "yauzl": "^2.8.0"
  }
}
