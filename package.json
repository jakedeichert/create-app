{
  "name": "@jakedeichert/create-app",
  "version": "1.0.6",
  "description": "A web app generator cli that abstracts away build configuration",
  "author": "Jake Deichert",
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "create-app": "bin/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "npm run _prettier:config -- --write '**/*.js'",
    "format:check": "npm run _prettier:config -- --list-different '**/*.js'",
    "_prettier:config": "prettier --single-quote --trailing-comma es5 --jsx-bracket-same-line"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "prettier": "^1.5.3",
    "request": "^2.81.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.1",
    "yargs": "^8.0.2",
    "yauzl": "^2.8.0"
  }
}
